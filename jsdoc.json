{
  "opts": {
    "destination": "build/doc",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["lib", "readme.md"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "staticFiles": {
        "include": [
          "./doc"
        ]
      }
    }
  }
}